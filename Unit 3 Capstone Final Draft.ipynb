    "# Finding the optimal parameters for Support Vector Regression\n",
    "svr = pd.DataFrame(columns = ('alpha', 'r_squared'))\n",
    "\n",
    "svr.epsilon = np.concatenate((np.arange(.1,1,.2), np.arange(1,300,2)), axis=0)\n",
    "\n",
